{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "exp",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.afea.smartwardrobe"
    },
    "android": {
      "package": "com.afea.smartwardrobe",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.supabase.co"
            },
            {
              "scheme": "exp"
            },
            {
              "scheme": "myapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", 
      "expo-font", 
      "expo-web-browser",
      [
        "expo-auth-session",
        {
          "schemes": ["exp", "myapp"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
